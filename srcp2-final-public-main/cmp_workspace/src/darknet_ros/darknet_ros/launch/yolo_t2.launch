<?xml version="1.0" encoding="utf-8"?>

<launch>

<!-- Console launch prefix -->

<arg name="launch_prefix" default=""/>

<!-- arg name="image" default="/small_hauler_1/camera/left/image_raw"/ -->
<arg name="image" default="/team_2/image_raw"/>

<!-- Config and weights folder. -->


<arg name="yolo_weights_path" default="$(find darknet_ros)/p"/>

<arg name="yolo_config_path" default="$(find darknet_ros)/p"/>

<!-- ROS and network parameter files -->

<arg name="ros_param_file" default="$(find darknet_ros)/config/0709_t2.yaml"/>

<arg name="network_param_file" default="$(find darknet_ros)/config/yolo.yaml"/>

<!-- Load parameters -->

<rosparam command="load" ns="darknet_ros_h2" file="$(arg ros_param_file)"/>

<rosparam command="load" ns="darknet_ros_h2" file="$(arg network_param_file)"/>

<!-- Start darknet and ros wrapper -->

<node pkg="darknet_ros" type="darknet_ros" name="darknet_ros_h2" output="screen" launch-prefix="$(arg launch_prefix)">

<param name="weights_path" value="$(arg yolo_weights_path)"/>

<param name="config_path" value="$(arg yolo_config_path)"/>

<remap from="camera/rgb/image_raw" to="$(arg image)"/>

</node>

<!--<node name="republish" type="republish" pkg="image_transport" output="screen" args="compressed in:=/camera/image raw out:=/camera/image_raw"/>-->


</launch>
